#include <stdio.h>
#include <string.h>


void main()
{
    char para[100];
    printf("enter the sentence");
    gets(para);
    int c=0;
    for(int i=0; i<strlen(para);i++)
    {
        if(para[i]=' '&&para[i+1]!= ' ')
        {
            c=c+1;
            printf(" the no of words are%d",c+1);
        }
        }
    }
